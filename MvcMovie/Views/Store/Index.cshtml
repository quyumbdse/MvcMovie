@model IEnumerable<MvcMovie.Models.Genre>
@{

    ViewBag.Title = "Store";
}
<h3>Browse Genres</h3>

<p>
    Select from @Model.GetType() genres:
</p>
<ul>
    <ul>
        @foreach (var genre in Model)
        {
            <li>@Html.ActionLink(genre.Name, "Browse", new { genre = genre.Name })</li>
        }
    </ul>

</ul>
